source "https://rubygems.org"

# GitHub Pages
gem "github-pages", group: :jekyll_plugins, "~> 232"

# Plugins
group :jekyll_plugins do
  gem "jekyll-feed"
  gem "jekyll-seo-tag"
  gem "jekyll-sitemap"
  gem "jekyll-paginate"
  gem "jekyll-include-cache"
end

# Windows and JRuby compatibility
gem "tzinfo", "~> 1.2"
gem "tzinfo-data"
